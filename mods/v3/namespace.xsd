	<xs:element name="mods" type="modsComplexType"/>	<!--  or (2) a collection of MODS records:  -->	<xs:element name="modsCollection" type="modsCollectionComplexType"/>	<!--  *****  End of "instance" definition******* Group definition. This forms the basis of the mods record definition, and also relatedItem. The difference between a MODS record and a relatedItem is that mods requires at least one element and relatedItem does not. The group definition is used by both, where relatedItem says minOccurs="0" and for the mods record definition minOccurs="1" (default).-->	<xs:group name="modsGroup">		<xs:choice>			<!-- *************************************************************************        These are the "top level" MODS elements               ************************************************************************-->			<xs:element ref="abstract"/>			<xs:element ref="accessCondition"/>			<xs:element ref="classification"/>			<xs:element ref="extension"/>			<xs:element ref="genre"/>			<xs:element ref="identifier"/>			<xs:element ref="language"/>			<xs:element ref="location"/>			<xs:element ref="name"/>			<xs:element ref="note"/>			<xs:element ref="originInfo"/>			<xs:element ref="part"/>			<xs:element ref="physicalDescription"/>			<xs:element ref="recordInfo"/>			<xs:element ref="relatedItem"/>			<xs:element ref="subject"/>			<xs:element ref="tableOfContents"/>			<xs:element ref="targetAudience"/>			<xs:element ref="titleInfo"/>			<xs:element ref="typeOfResource"/>			<!-- End list of "top level" MODS elements -->		</xs:choice>	</xs:group>	<!-- *************************************************************************      Definition of a single MODS record                           ************************************************************************-->	<xs:complexType name="modsComplexType">		<xs:group ref="modsGroup" maxOccurs="unbounded"/>		<xs:attribute name="ID" type="xs:ID"/>		<xs:attribute name="version" type="versionSimpleType"/>	</xs:complexType><!-- *************************************************************************      Definition of a MODS collection                                ************************************************************************-->	<xs:complexType name="modsCollectionComplexType">		<xs:sequence>			<xs:element ref="mods" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--	**********************************************************************************************************************************************************************************                                  Type Definitions  -   Data type definitions for top level elements                                   -   subordinate type definitions                                  	**********************************************************************************************************************************************************************************All type names are part of the MODS namespace                                  	*************************************************************************       Data type definitions for top level elements              *************************************************************************-->	<!--   ********** abstractComplexType   **********               -->	<xs:complexType name="abstractComplexType">		<xs:simpleContent>			<xs:extension base="unstructuredTextComplexType"/>		</xs:simpleContent>	</xs:complexType>	<!--********** accessConditionComplexType   **********            -->	<xs:complexType name="accessConditionComplexType">		<xs:complexContent>			<xs:extension base="extensionType">				<xs:attributeGroup ref="xlink:simpleLink"/>				<xs:attributeGroup ref="language"/>				<xs:attribute name="displayLabel" type="xs:string"/>				<xs:attribute name="type" type="xs:string"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<!--**********   classificationComplexType  **********  	 -->	<xs:complexType name="classificationComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusAuthorityPlusEdition">				<xs:attributeGroup ref="language"/>				<xs:attribute name="displayLabel" type="xs:string"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- ********** extensionType  **********  -->	<xs:complexType name="extensionType" mixed="true">		<xs:sequence>			<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--  ********** genreComplexType definition  **********  	-->	<xs:complexType name="genreComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusAuthorityPlusTypePlusLanguage"/>		</xs:simpleContent>	</xs:complexType>	<!--********** identifierComplexType   **********  	                     	-->	<xs:complexType name="identifierComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="type" type="xs:string"/>				<xs:attribute name="displayLabel" type="xs:string"/>				<xs:attribute name="invalid" type="yesSimpleType"/>				<xs:attributeGroup ref="language"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--********** locationComplexType   **********               -->	<xs:complexType name="locationComplexType">		<xs:sequence>			<xs:element ref="physicalLocation" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="shelfLocator" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="url" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="holdingSimple" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="holdingExternal" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--  **********  nameComplexType    **********   		-->	<xs:complexType name="nameComplexType">		<xs:choice minOccurs="0" maxOccurs="unbounded">			<xs:element ref="namePart"/>			<xs:element ref="displayForm"/>			<xs:element ref="affiliation"/>			<xs:element ref="role"/>			<xs:element ref="description"/>		</xs:choice>		<xs:attribute name="type" type="nameTypeAttributeSimpleType"/>		<xs:attributeGroup ref="idAuthorityXlinkLanguage"/>	</xs:complexType>	<!--  ********** languageComplexType   **********  	                      	-->	<xs:complexType name="languageComplexType">		<xs:sequence>			<xs:element ref="languageTerm" maxOccurs="unbounded">				</xs:element>		</xs:sequence>		<xs:attribute name="objectPart" type="xs:string"/>	</xs:complexType>		<!--      ********** nameTypeAttributeSimpleType  used by name attribute       -->	<xs:simpleType name="nameTypeAttributeSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="personal">			</xs:enumeration>			<xs:enumeration value="corporate">			</xs:enumeration>			<xs:enumeration value="conference">			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<!--   ********** originInfoComplexType   ********** -->	<xs:complexType name="originInfoComplexType">		<xs:choice maxOccurs="unbounded">			<xs:element ref="place"/>			<xs:element ref="publisher"/>			<xs:element ref="dateIssued"/>			<xs:element ref="dateCreated"/>			<xs:element ref="dateCaptured"/>			<xs:element ref="dateValid"/>			<xs:element ref="dateModified"/>			<xs:element ref="copyrightDate"/>			<xs:element ref="dateOther"/>			<xs:element ref="edition"/>			<xs:element ref="frequency"/>			<xs:element ref="issuance"/>		</xs:choice>		<xs:attributeGroup ref="language"/>	</xs:complexType>	<!--********** partComplexType  **********  -->	<xs:complexType name="partComplexType">		<xs:choice minOccurs="0" maxOccurs="unbounded">			<!--minOccurs="0" allows <part> to be optionally empty,   when the ID and type attribute values contain all the information needed about the part. -->			<xs:element ref="detail"/>			<xs:element ref="extent"/>			<xs:element ref="date"/>			<xs:element ref="text"/>		</xs:choice>		<xs:attribute name="ID" type="xs:ID"/>		<xs:attribute name="type" type="xs:string"/>		<xs:attribute name="order" type="xs:integer"/>	</xs:complexType>	<!--   ********** noteComplexType   **********               -->	<xs:complexType name="noteComplexType">		<xs:simpleContent>			<xs:extension base="unstructuredTextComplexType">				<xs:attribute name="ID" type="xs:ID"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- ********** physicalDescriptionComplexType  **********  		 		 -->	<xs:complexType name="physicalDescriptionComplexType">		<xs:choice maxOccurs="unbounded">			<xs:element ref="form"/>			<xs:element ref="reformattingQuality"/>			<xs:element ref="internetMediaType"/>			<xs:element name="extent" type="xs:string"/>			<!-- <extent> as a subelement of <physicalDescription>  is not part of the MODS namespace. That's because it conflicts with  <extent>  as subelement of <part>.                          <extent> as a subelement of <physicalDescription>  is retained for compatibility, but <physicalExtent> is added in 3.4 -->			<xs:element ref="physicalExtent"/>			<!-- ****** new in3.4. See above. -->			<xs:element ref="digitalOrigin"/>			<xs:element ref="note"/>		</xs:choice>		<xs:attributeGroup ref="language"/>	</xs:complexType>	<!-- ********** recordIdentifierComplexType  **********  -->	<xs:complexType name="recordIdentifierComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="source" type="xs:string"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--  **********  recordInfoComplexType  **********   -->	<xs:complexType name="recordInfoComplexType">		<xs:choice maxOccurs="unbounded">			<xs:element ref="recordContentSource"/>			<xs:element ref="recordCreationDate"/>			<xs:element ref="recordChangeDate"/>			<xs:element ref="recordIdentifier"/>			<xs:element ref="languageOfCataloging"/>			<xs:element ref="recordOrigin"/>			<xs:element ref="descriptionStandard"/>		</xs:choice>		<xs:attributeGroup ref="language"/>	</xs:complexType>	<!-- **********   relatedItemComplexType  **********  -->	<xs:complexType name="relatedItemComplexType">		<xs:group ref="modsGroup" minOccurs="0" maxOccurs="unbounded"/>			<xs:attribute name="type">			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value="preceding"/>					<xs:enumeration value="succeeding"/>					<xs:enumeration value="original"/>					<xs:enumeration value="host"/>					<xs:enumeration value="constituent"/>					<xs:enumeration value="series"/>					<xs:enumeration value="otherVersion"/>					<xs:enumeration value="otherFormat"/>					<xs:enumeration value="isReferencedBy"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="displayLabel" type="xs:string"/>		<xs:attribute name="ID" type="xs:ID"/>		<xs:attributeGroup ref="xlink:simpleLink"/>	</xs:complexType>	<!--   ********** subjectComplexType   **********  	 	-->	<xs:complexType name="subjectComplexType">		<xs:choice minOccurs="0" maxOccurs="unbounded">			<xs:element ref="topic"/>			<xs:element ref="geographic"/>			<xs:element ref="temporal"/>			<xs:element ref="titleInfo"/>			<xs:element ref="name"/>			<xs:element ref="geographicCode"/>			<xs:element ref="hierarchicalGeographic"/>			<xs:element ref="cartographics"/>			<xs:element ref="occupation"/>			<xs:element ref="genre"/>		</xs:choice>		<xs:attributeGroup ref="idAuthorityXlinkLanguage"/>	</xs:complexType>	<!--   ********** tableOfContentsComplexType   **********               -->	<xs:complexType name="tableOfContentsComplexType">		<xs:simpleContent>			<xs:extension base="unstructuredTextComplexType"/>		</xs:simpleContent>	</xs:complexType>	<!--   ********** targetAudienceComplexType   **********               -->	<xs:complexType name="targetAudienceComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusAuthorityPlusLanguage"/>		</xs:simpleContent>	</xs:complexType>	<!-- ********** titleInfoComplexType  ********** -->	<xs:complexType name="titleInfoComplexType">		<xs:complexContent>			<xs:extension base="baseTitleInfoComplexType">				<xs:attribute name="type">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="abbreviated">							</xs:enumeration>							<xs:enumeration value="translated">							</xs:enumeration>							<xs:enumeration value="alternative">							</xs:enumeration>							<xs:enumeration value="uniform">							</xs:enumeration>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<!--********** typeOfResourceComplexType   **********   	   -->	<xs:complexType name="typeOfResourceComplexType">		<xs:simpleContent>			<xs:extension base="resourceTypeSimpleType">				<xs:attribute name="collection" type="yesSimpleType"/>				<xs:attribute name="manuscript" type="yesSimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--  ***** End  data type definitions for top level elements.   *************************************************************************     Subordinate Type Definitions                                      *************************************************************************-->	<!--  ********** baseDateComplexType   **********  Referenced by partComplexType (as well as dateType)-->	<xs:complexType name="baseDateComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="encoding">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="w3cdtf"/>							<xs:enumeration value="iso8601"/>							<xs:enumeration value="marc"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>				<xs:attribute name="qualifier">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="approximate"/>							<xs:enumeration value="inferred"/>							<xs:enumeration value="questionable"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>				<xs:attribute name="point">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="start"/>							<xs:enumeration value="end"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- ********** baseTitleInfoComplexType  **********  -->	<xs:complexType name="baseTitleInfoComplexType">		<xs:choice minOccurs="0" maxOccurs="unbounded">			<xs:element ref="title"/>			<xs:element ref="subTitle"/>			<xs:element ref="partNumber"/>			<xs:element ref="partName"/>			<xs:element ref="nonSort"/>		</xs:choice>		<xs:attribute name="displayLabel" type="xs:string"/>		<xs:attributeGroup ref="idAuthorityXlinkLanguage"/>	</xs:complexType>	<!--  ********** dateOtherComplexType  **********  -->	<xs:complexType name="dateOtherComplexType">		<xs:simpleContent>			<xs:extension base="dateType">				<xs:attribute name="type" type="xs:string"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--   ********** cartographicsComplexType **********  	 	-->	<xs:complexType name="cartographicsComplexType">		<xs:sequence>			<xs:element ref="scale" minOccurs="0"/>			<xs:element ref="projection" minOccurs="0"/>			<xs:element ref="coordinates" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--     ********** codeOrTextSimpleType used by type attribute  for elements that distinguish code from text  **********  	                       	   -->	<xs:simpleType name="codeOrTextSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="code"/>			<xs:enumeration value="text"/>		</xs:restriction>	</xs:simpleType>	<!--**********copyInformationComplexType  **********               -->	<xs:complexType name="copyInformationComplexType">		<xs:sequence>			<xs:element ref="form" minOccurs="0"/>			<xs:element ref="subLocation" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="shelfLocator" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="electronicLocator" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>			<!-- changed in 3.4.  In 3.3 <note> as a sublement of <copyInformation> was defined as type "stringPlusDisplayLabelPlusType". Here it is changed to conform to the note element defined for the MODS namespace.   The effect of this change is that attributes xlink, language, and ID	are now allowed - they were not allowed in earlier versions. Since they are optional this will not affect existing instances.			-->			<xs:element ref="enumerationAndChronology" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--  ********** dateType   **********  -->	<xs:complexType name="dateType">		<xs:simpleContent>			<xs:extension base="baseDateComplexType">				<xs:attribute name="keyDate" type="yesSimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- ********** detailComplexType  **********  -->	<xs:complexType name="detailComplexType">		<xs:choice maxOccurs="unbounded">			<xs:element ref="number"/>			<xs:element ref="caption"/>			<xs:element ref="title"/>		</xs:choice>		<xs:attribute name="type" type="xs:string"/>		<xs:attribute name="level" type="xs:positiveInteger"/>	</xs:complexType>	<!--********** digitalOriginSimpleType **********  		 		 -->	<xs:simpleType name="digitalOriginSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="born digital"/>			<xs:enumeration value="reformatted digital">						</xs:enumeration>			<xs:enumeration value="digitized microfilm">						</xs:enumeration>			<xs:enumeration value="digitized other analog">						</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<!--  **********enumerationAndChronologyComplexType  **********               -->	<xs:complexType name="enumerationAndChronologyComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="unitType">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="1">							</xs:enumeration>							<xs:enumeration value="2">							</xs:enumeration>							<xs:enumeration value="3">							</xs:enumeration>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--***************** extentComplexType *** -->	<xs:complexType name="extentComplexType">		<xs:sequence>			<xs:element ref="start" minOccurs="0"/>			<xs:element ref="end" minOccurs="0"/>			<xs:element ref="total" minOccurs="0"/>			<xs:element ref="list" minOccurs="0"/>		</xs:sequence>		<xs:attribute name="unit"/>	</xs:complexType>	<!--   ********** geographicCodeComplexType **********  	 	-->	<xs:complexType name="geographicCodeComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="authority" type="placeAuthoritySimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--   ********** hierarchicalGeographicComplexType **********  	 	-->	<xs:complexType name="hierarchicalGeographicComplexType">		<xs:choice maxOccurs="unbounded">			<xs:element ref="extraterrestrialArea"/>			<xs:element ref="continent"/>			<xs:element ref="country"/>			<xs:element ref="province"/>			<xs:element ref="region"/>			<xs:element ref="state"/>			<xs:element ref="territory"/>			<xs:element ref="county"/>			<xs:element ref="city"/>			<xs:element ref="citySection"/>			<xs:element ref="island"/>			<xs:element ref="area"/>		</xs:choice>	</xs:complexType>	<!--  ********** holdingSimpleComplexType   **********               -->	<xs:complexType name="holdingSimpleComplexType">		<xs:sequence>			<xs:element ref="copyInformation" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!--  ********** issuanceSimpleType **********  	-->	<xs:simpleType name="issuanceSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="continuing"/>			<xs:enumeration value="monographic"/>		</xs:restriction>	</xs:simpleType>	<!-- ***languageTermComplexType ***-->	<xs:complexType name="languageTermComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="authority">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="rfc3066"/>							<xs:enumeration value="iso639-2b"/>							<xs:enumeration value="iso639-3"/>							<xs:enumeration value="rfc4646"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>				<xs:attribute name="type" type="codeOrTextSimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--  **namePartComplexType-->	<xs:complexType name="namePartComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="type">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="date"/>							<xs:enumeration value="family"/>							<xs:enumeration value="given"/>							<xs:enumeration value="termsOfAddress"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--      ********** placeAuthoritySimpleType used by authority attribute   for  placeComplexType and geographic  **********  	                       	   -->	<xs:simpleType name="placeAuthoritySimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="marcgac"/>			<xs:enumeration value="marccountry"/>			<xs:enumeration value="iso3166"/>		</xs:restriction>	</xs:simpleType>	<!-- *** placeComplexType ***-->	<xs:complexType name="placeComplexType">		<xs:sequence>			<xs:element ref="placeTerm" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<!-- *** placeTermComplexType ***-->	<xs:complexType name="placeTermComplexType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="authority" type="placeAuthoritySimpleType"/>				<xs:attribute name="type" type="codeOrTextSimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--	********** physicalExtentComplexType   **********            -->	<xs:complexType name="physicalExtentComplexType">		<!--  placeholder for future definition -->	</xs:complexType>	<!--    ********** physicalLocationComplexType    **********               -->	<xs:complexType name="physicalLocationComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusAuthorityPlusTypePlusLanguagePlusDisplayLabelPlusXlink">			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- **********reformattingQualitySimpleType **********  		 		 -->	<xs:simpleType name="reformattingQualitySimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="access"/>			<xs:enumeration value="preservation"/>			<xs:enumeration value="replacement"/>		</xs:restriction>	</xs:simpleType>	<!--  ******* resourceTypeSimpleType  ********     	   -->	<xs:simpleType name="resourceTypeSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="text"/>			<xs:enumeration value="cartographic"/>			<xs:enumeration value="notated music"/>			<xs:enumeration value="sound recording-musical"/>			<xs:enumeration value="sound recording-nonmusical"/>			<xs:enumeration value="sound recording"/>			<xs:enumeration value="still image"/>			<xs:enumeration value="moving image"/>			<xs:enumeration value="three dimensional object"/>			<xs:enumeration value="software, multimedia"/>			<xs:enumeration value="mixed material"/>			<xs:enumeration value=""/>		</xs:restriction>	</xs:simpleType>	<!--  ******** roleComplexType *********************-->	<xs:complexType name="roleComplexType">		<xs:sequence maxOccurs="unbounded">			<xs:element ref="roleTerm"/>		</xs:sequence>	</xs:complexType>	<!--  ***************roleTermComplexType ***********************-->	<xs:complexType name="roleTermComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusAuthority">				<xs:attribute name="type" type="codeOrTextSimpleType"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--********** unstructuredTextComplexType ********** -->	<xs:complexType name="unstructuredTextComplexType">		<xs:simpleContent>			<xs:extension base="stringPlusDisplayLabelPlusType">				<xs:attributeGroup ref="xlink:simpleLink">				</xs:attributeGroup>				<xs:attributeGroup ref="language"/>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!-- ********** urlComplexType   **********               -->	<xs:complexType name="urlComplexType">		<xs:simpleContent>			<xs:extension base="xs:anyURI">				<xs:attribute name="dateLastAccessed" type="xs:string"/>				<xs:attribute name="displayLabel" type="xs:string"/>				<xs:attribute name="note" type="xs:string"/>				<xs:attribute name="access">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="preview"/>							<xs:enumeration value="raw object"/>							<xs:enumeration value="object in context"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>				<xs:attribute name="usage">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="primary display"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<!--	 ********** versionSimpleType **********  	                       	   -->	<xs:simpleType name="versionSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="3.3"/>		</xs:restriction>	</xs:simpleType>	<!--   ********** yesSimpleType **********  	   -->	<xs:simpleType name="yesSimpleType">		<xs:restriction base="xs:string">			<xs:enumeration value="yes"/>		</xs:restriction>	</xs:simpleType><!--**********